<?xml version="1.0" encoding="UTF-8"?>
<StructureDefinition
    xmlns="http://hl7.org/fhir">
    <id value="Condition-fetal-death-other-cause-or-condition"/>
    <meta>
        <versionId value="9"/>
        <lastUpdated value="2021-04-21T18:53:54.399+00:00"/>
