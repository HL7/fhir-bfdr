    </meta>
    <url value="http://hl7.org/fhir/us/bfdr/StructureDefinition/Condition-fetal-death-other-cause-or-condition"/>
    <version value="1.0.0"/>
    <name value="ConditionFetalDeathOtherCauseOrCondition"/>
    <title value="Condition - Fetal Death Other Cause or Condition"/>
    <status value="active"/>
    <experimental value="false"/>
    <publisher value="HL7 Public Health Work Group"/>
    <description value="This Condition profile represents another significant cause or condition for the death of the fetus. The type of cause or condition (code.coding) should be selected from the value set Fetal Death Cause or Condition and any further specification should be entered as free text (code.text) to fully record the provider's view of the cause."/>
    <jurisdiction>
        <coding>
            <system value="urn:iso:std:iso:3166"/>
            <code value="US"/>
            <display value="United States of America"/>
        </coding>
    </jurisdiction>
    <fhirVersion value="4.0.1"/>
    <kind value="resource"/>
    <abstract value="false"/>
    <type value="Condition"/>
    <baseDefinition value="http://hl7.org/fhir/us/core/StructureDefinition/us-core-condition"/>
    <derivation value="constraint"/>
    <differential>
        <element id="Condition">
            <path value="Condition"/>
            <short value="Another significant cause or condition for the death of the fetus."/>
        </element>
        <element id="Condition.category">
            <path value="Condition.category"/>
            <short value="Other significant causes or conditions of fetal death [US Standard Report of Fetal Death]"/>
            <patternCodeableConcept>
                <coding>
                    <system value="http://loinc.org"/>
                    <code value="76061-1"/>
                </coding>
            </patternCodeableConcept>
        </element>
        <element id="Condition.code">
            <path value="Condition.code"/>
            <short value="Another significant cause or condition of the fetal death"/>
            <binding>
                <strength value="required"/>
                <description value="The list of values used to report initiating cause or condition or other significant causes or conditions contributing to fetal death."/>
                <valueSet value="http://phinvads.cdc.gov/fhir/ValueSet/2.16.840.1.114222.4.11.7422"/>
            </binding>
        </element>
        <element id="Condition.code.coding">
            <path value="Condition.code.coding"/>
            <short value="The type of cause or condition should be selected from the bound value set."/>
            <mustSupport value="true"/>
        </element>
        <element id="Condition.code.text">
            <path value="Condition.code.text"/>
            <short value="Any further specification should be entered as free text to fully record the provider's view of the cause."/>
            <mustSupport value="true"/>
        </element>
    </differential>
</StructureDefinition>