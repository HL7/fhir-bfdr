            <path value="Observation"/>
        </element>
        <element id="Observation.code">
            <path value="Observation.code"/>
            <short value="Number of Prenatal Care Visits edit flag"/>
            <min value="1"/>
            <max value="1"/>
            <patternCodeableConcept>
                <coding>
                    <system value="urn:oid:2.16.840.1.114222.4.5.274"/>
