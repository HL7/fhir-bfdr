            <path value="Composition.type"/>
            <short value="Provider supplied mother's fetal death report Document"/>
            <definition value="Provider supplied mother's fetal death report Document"/>
            <patternCodeableConcept>
                <coding>
                    <system value="http://loinc.org"/>
                    <code value="92019-9"/>
                </coding>
            </patternCodeableConcept>
        </element>
        <element id="Composition.subject">
